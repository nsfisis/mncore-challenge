16.times do |mabid|
  4.times do |subpeid|
    peid = mabid*4 + subpeid
    i = peid >> 3               # $lr[0]
    j = peid & 0b111            # $lr[2]
    j1 = j >> 2                 # $lr[4]
    j2 = j & 0b11               # $lr[6]
    lm = (((j1 + (j2 << 1)) << 3) + i) << 1
    puts "MAB #{mabid} PE #{subpeid} (#{peid}): $lm[#{lm}]"
  end
end


